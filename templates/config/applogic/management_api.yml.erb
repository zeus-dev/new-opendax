keychain:
  barong:
    algorithm: RS256
    value: <%= @barong_public_key %>
  peatio:
    algorithm: RS256
    value: <%= @barong_public_key %>
    
scopes:
  read_codes:
    mandatory_signers:
      - barong
    permitted_signers:
      - barong
  write_codes:
    mandatory_signers:
      - barong
    permitted_signers:
      - barong
