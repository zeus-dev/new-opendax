path "transit/test_*" {
  capabilities = [ "read" ]
}
# Decrypt secrets
path "transit/decrypt/test_*" {
  capabilities = [ "create", "update" ]
}
# Use key for signing
path "transit/sign/test_*" {
  capabilities = ["update"]
}
# Create transit key
path "transit/keys/test_*" {
  capabilities = ["create"]
}
# Renew tokens
path "auth/token/renew" {
  capabilities = ["update"]
}
# Lookup tokens
path "auth/token/lookup" {
  capabilities = ["update"]
}
